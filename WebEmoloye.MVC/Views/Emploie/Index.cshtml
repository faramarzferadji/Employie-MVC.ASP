@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}


<table class="table table-bordered table-striped">
    <tr>
        <th>Emploie ID</th>
        <th>Full Name</th>
        <th>Age</th>
        <th>Seniority</th>
        <th>Work Section</th>
        <th>Work Position</th>
        <th>Address</th>
        <th>Telephone</th>
        <th>Email</th>
        <th></th>

    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
        <tr>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][4]</td>
            <td>@Model.Rows[i][5]</td>
            <td>@Model.Rows[i][6]</td>
            <td>@Model.Rows[i][7]</td>
            <td>@Model.Rows[i][8]</td>
            <td>@Model.Rows[i][9]</td>
            <td>
                <a href="@Url.Action("Edit","Emploie",new { @id = Model.Rows[i][0] })"><Button type="button" class="arrow">Edit<i class="glyphicon glyphicon-pencil"></i></Button></a>

                @*<a href="@Url.Action("Delete","Emploie",new { @id = Model.Rows[i][0] })"><Button type="button" class="btn-danger">Delete  &Chi;</Button></a>*@
                <a href="@Url.Action("Delete","Emploie",new { @id = Model.Rows[i][0] })"><Button type="button" class="btn-danger">Delete <i class="glyphicon glyphicon-trash"></i></Button></a>
            </td>
        </tr>

    }

</table>
<a href="@Url.Action("Create","Emploie")"><Button type="button" class="btn-primary"><i class="glyphicon glyphicon-asterisk"></i>Add Data</Button></a>